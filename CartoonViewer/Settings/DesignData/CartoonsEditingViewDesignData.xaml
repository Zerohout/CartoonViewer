    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:vms="clr-namespace:CartoonViewer.Settings.ViewModels"
    Name="CartoonName"
    CartoonTypesVisibility="Visible"
    Description="Небольшое, но информативное описание мультфильма"
    SaveChangesVisibility="Visible"
    Url="http://cartoonUrl.url">
