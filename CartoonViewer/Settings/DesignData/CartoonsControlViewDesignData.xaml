<vms:CartoonsControlViewModel xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                         xmlns:cartoonModels="clr-namespace:CartoonViewer.Models.CartoonModels"
                         xmlns:vms="clr-namespace:CartoonViewer.Settings.ViewModels"
                              CartoonsVisibility="Visible"
                              SeasonsVisibility="Visible">

    <vms:CartoonsControlViewModel.WebSites>
        <cartoonModels:WebSite Url="http://freehat.cc"/> 
    </vms:CartoonsControlViewModel.WebSites>

    <vms:CartoonsControlViewModel.Cartoons>
        <cartoonModels:Cartoon Name="Южный парк"/>
        <cartoonModels:Cartoon Name="Гриффины"/>
        <cartoonModels:Cartoon Name="Симпсоны"/>
        <cartoonModels:Cartoon Name="Американский папаша"/>
    </vms:CartoonsControlViewModel.Cartoons>

    <vms:CartoonsControlViewModel.Seasons>
        <cartoonModels:Season Number="1"/>
        <cartoonModels:Season Number="2"/>
        <cartoonModels:Season Number="3"/>
        <cartoonModels:Season Number="4"/>
        <cartoonModels:Season Number="5"/>
    </vms:CartoonsControlViewModel.Seasons>
    
</vms:CartoonsControlViewModel>